{
  "extends": ["tslint-microsoft-contrib"],
  "rules": {
    "missing-jsdoc": false,
    "variable-name": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "export-name": false,
    "match-default-export-name": true,
    "no-import-side-effect": [true, {"ignore-module": "(\\.html|\\.css|\\.scss)$"}],
    "no-relative-imports": false,
    "import-name": false,
    "typedef": [
      true, "call-signature", "arrow-call-signature",
      "parameter", "arrow-parameter", 
      "property-declaration", "member-variable-declaration",
    ]
  }
}